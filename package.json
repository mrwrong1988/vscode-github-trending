{
	"name": "github-trending",
	"displayName": "Github Trending",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:nodeRepos",
		"onCommand:githubTrending.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "githubTrending.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "nodeRepos.refreshEntry",
				"title": "Refresh",
				"icon": {
					"light": "resources/light/refresh.svg",
					"dark": "resources/dark/refresh.svg"
				}
			},
			{
				"command": "nodeRepos.since",
				"title": "Since",
				"icon": {
					"light": "resources/light/since.svg",
					"dark": "resources/dark/since.svg"
				}
			},
			{
				"command": "nodeRepos.language",
				"title": "Language",
				"icon": {
					"light": "resources/light/language.svg",
					"dark": "resources/dark/language.svg"
				}
			},
			{
				"command": "nodeRepos.spokenLanguage",
				"title": "Spoken Language",
				"icon": {
					"light": "resources/light/spoken_language.svg",
					"dark": "resources/dark/spoken_language.svg"
				}
			},
			{
				"command": "nodeRepos.view",
				"title": "View"
			},
			{
				"command": "nodeRepos.addToFavorites",
				"title": "Add To Favorites",
				"icon": {
					"light": "resources/light/spoken_language.svg",
					"dark": "resources/dark/spoken_language.svg"
				}
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "githubTrending",
					"title": "Github-Trending",
					"icon": "resources/trend.svg"
				}
			]
		},
		"views": {
			"githubTrending": [
				{
					"id": "nodeRepos",
					"name": "Repo-Tredding"
				},
				{
					"id": "nodeUsers",
					"name": "User-Tredding"
				}
			]
		},
		"menus": {
			"view/title": [
				{
					"command": "nodeRepos.since",
					"when": "view == nodeRepos",
					"group": "navigation@1"
				},
				{
					"command": "nodeRepos.language",
					"when": "view == nodeRepos",
					"group": "navigation@2"
				},
				{
					"command": "nodeRepos.spokenLanguage",
					"when": "view == nodeRepos",
					"group": "navigation@3"
				},
				{
					"command": "nodeRepos.refreshEntry",
					"when": "view == nodeRepos",
					"group": "navigation@4"
				}
			],
			"view/item/context": [
				{
					"command": "nodeRepos.addToFavorites",
					"when": "viewItem == repo",
					"group": "inline"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/node-fetch": "^2.5.7",
		"@types/vscode": "^1.46.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"amplitude": "^5.1.2",
		"event-emitter-enhancer": "^2.0.0",
		"eventemitter2": "^6.4.3",
		"get-port": "^5.1.1",
		"karma-chrome-launcher": "^3.1.0",
		"mixpanel": "^0.11.0",
		"node-fetch": "^2.6.0",
		"os-name": "^3.1.0",
		"public-ip": "^4.0.1",
		"puppeteer-core": "^5.0.0",
		"uuid": "^3.3.2",
		"which-chrome": "^0.1.1"
	}
}
